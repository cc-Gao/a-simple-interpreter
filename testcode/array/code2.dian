@*
	数组间类型转换（给的output跟自己代码跑的不太一样。。）
*@
int a[16];
int b[16];
int c[16];
int len;
int i;
a[0]=4;
a[1]=3456;
a[2]=9012;
a[3]=5678;
a[4]=1234;
b[0]=4;
b[1]=3456;
b[2]=9012;
b[3]=5678;
b[4]=1234;
len=4;
for i=1; i<=len; i=i+1{
	c[i]=a[i]+b[i];
	c[i+1]+=c[i]/10000;
	c[i]%=10000;
}
if c[len+1]>0{
	c[0]=len+1;
}else{
	c[0]-len;
}
print("{c[4]}{c[3]}{c[2]}{c[1]}");