@*
	函数作为参数传递
*@
func max(int a, int b) int{
	if a>b{
		return a;
	}
	else{
		return b;
	}
}
func min(int a, int b) int{
	if a<b{
		return a;
	}
	else{
		return b;
	}
}
func calc(int a, int b, func f(int, int)int) int{
	return f(a,b);
}
int a,b;
a,b=readi(),readi();
print("max(a,b)={calc(a,b,max)}, min(a,b)={calc(a,b,min)}");

